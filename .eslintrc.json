{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "rules": {
        "require-jsdoc": "off",
        "object-curly-spacing": 0,
        "no-invalid-this": 0,
        "block-spacing": 0,
        "semi": 0,
        "arrow-parens": 0,
        "linebreak-style": 0
    }
}
